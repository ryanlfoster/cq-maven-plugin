          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>webDavServerPath</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
      </parameters>
      <configuration>
        <delayMax implementation="java.lang.Long" default-value="100000"/>
        <felixConsolePath implementation="java.lang.String" default-value="/system/console/bundles"/>
        <jarFile implementation="java.io.File" default-value="${project.build.directory}/${project.artifactId}-${project.version}.jar"/>
        <outputDirectory implementation="java.io.File" default-value="${project.build.outputDirectory}"/>
        <resources implementation="java.util.List" default-value="${project.resources}"/>
        <syncRefFile implementation="java.io.File" default-value="${project.build.directory}/syncRefFile.xml"/>
        <syncSrcFile implementation="java.io.File" default-value="${project.build.directory}/classes/jcr_root"/>
        <timeRefFile implementation="java.io.File" default-value="${project.build.outputDirectory}/timeref"/>
      </configuration>
